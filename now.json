{
  "version": 2,
  "name": "plaza_backend",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "alias": [
    "plza-backend.now.sh"
  ],
  "env":{
    "NODE_ENV": "production",
    "PORT": "1337",
    "DATABASE_ATLAS_URL": "@log-db",
    "CORS_ORIGIN":"http://localhost:3000",
    "CORS_CLIENT":"http://localhost:4200",
    "API_KEY":""
  }
}